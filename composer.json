{
    "name": "orynider/custom_headernfo",
    "type": "phpbb-extension",
    "description": "This extension allows you to insert custom text in the header bar. Fully configurable from ACP.",
    "homepage": "http://mxpcms.sourceforge.net",
    "version": "0.9.9",
    "time": "2019-04-10",
    "keywords": ["phpbb", "extension", "custom", "header"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "FlorinCB",
            "email": "orynider@users.sourceforge.net",
            "homepage": "http://mxpcms.sourceforge.net",
            "role": "Extension Developer"
        },
        {
            "name": "Galandas",
            "email": "manzarek@live.it",
            "homepage": "http://phpbb3world.altervista.org",
            "role": "Credit to the author of bannerscroll phpBB extension"
        }
    ],
    "require": {
        "php": ">=5.4",
        "phpbb/phpbb": ">=3.2.0"
    },
    "extra": {
        "display-name": "Custom Header Info",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/custom_headernfo",
            "filename": "version_check"
        }
    }
}